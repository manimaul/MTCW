import os.path
import shutil

p = os.path.abspath('/home/will/charts/NOAA_BSB_REGION_ALL/')

#print os.listdir(p)
num = 0;
for ea in os.listdir(p):
    np = os.listdir(p+'/'+ea)

    if np.count('14')>0 and np.count('13')>0 and np.count('12')>0:
        deldir = p+'/'+ea
        shutil.rmtree(deldir)
        print "deleting: " + ea
        num += 1

print num
